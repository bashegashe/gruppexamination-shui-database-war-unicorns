{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "consistent-return": "off",
        "no-restricted-syntax": "off",
        "no-await-in-loop": "off",
        "no-plusplus": "off",
        "func-names": "off"
    }
}
